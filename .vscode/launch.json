{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node.js 单文件调试",
            "program": "${workspaceFolder}/${file}",
            "skipFiles": ["<node_internals>/**"]
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}\\server"
        }
    ]
}
