      "intro": "",
      "controlIntensity": "S",
      "applyObjectType": "P",
      "applyFieldType": "index",
      "programCode": "if(data.index.unique){\n    return data.index.defKey.indexOf(\"uk_\")==0;\n}else{\n    return data.index.defKey.indexOf(\"ix_\")==0;\n}",
      "enable": true
    },
